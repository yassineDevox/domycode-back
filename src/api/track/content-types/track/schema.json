{
  "kind": "collectionType",
  "collectionName": "tracks",
  "info": {
    "singularName": "track",
    "pluralName": "tracks",
    "displayName": "Track",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": false,
      "unique": true,
      "minLength": 5,
      "maxLength": 100
    },
    "description": {
      "type": "text",
      "required": true,
      "maxLength": 300,
      "minLength": 5
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "active": {
      "type": "boolean",
      "default": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "tracks"
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::student.student",
      "inversedBy": "tracks"
    },
    "instructors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::instructor.instructor",
      "inversedBy": "tracks"
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter"
    }
  }
}
